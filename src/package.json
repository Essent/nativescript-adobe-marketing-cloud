{
    "name": "nativescript-adobe-experience-cloud",
    "version": "2.0.0",
    "description": "Nativescript Adobe experience cloud integration plugin",
    "main": "adobe-analytics",
    "typings": "index.d.ts",
    "nativescript": {
        "platforms": {
            "android": "6.0.0",
            "ios": "6.0.1"
        }
    },
    "scripts": {
        "tsc": "npm i && tsc",
        "build": "npm run tsc && ts-patch install && tsc -skipLibCheck",
        "tslint": "cd .. && tslint \"**/*.ts\" --config tslint.json --exclude \"**/node_modules/**\"",
        "ci.tslint": "npm i && tslint '**/*.ts' --config '../tslint.json' --exclude '**/node_modules/**' --exclude '**/platforms/**'",
        "demo.ios": "npm run build && cd ../demo && ns run ios",
        "demo.android": "npm run build && cd ../demo && ns run android",
        "demo.reset": "cd ../demo && npx rimraf -- hooks node_modules platforms package-lock.json",
        "plugin.prepare": "npm run build && cd ../demo && tns plugin remove nativescript-adobe-experience-cloud && tns plugin add ../src",
        "clean": "npm run demo.reset && npx rimraf -- node_modules package-lock.json && npm run setup",
        "setup": "npm i && ts-patch install && tsc"
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS",
        "Adobe",
        "Adobe Mobile SDK"
    ],
    "author": {
        "name": "Essent"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Essent/nativescript-adobe-marketing-cloud.git"
    },
    "contributors": [
        "Jasper Boeijenga <jasper.boeijenga@essent.nl>",
        "Henk Bakker <spike1292@gmail.com>",
        "Oscar Lodriguez <o.lodriguez@gmail.com>",
        "Robert Jan Van der Hulst <rvdhulst@quintor.nl>",
        "Steve Jackson <stevenljackson1@gmail.com>",
        "Corn√© de Bruin <corne.de.bruin@navara.nl>",
        "Natasa Vukovic <n.vukovic@levi9.com>",
        "Brane Ivanovic <b.ivanovic@levi9.com>",
        "Djordje Stojanovic <dj.stojanovic@levi9.com>",
        "Petar Bozin <pecila@gmail.com>"
    ],
    "license": "MIT",
    "homepage": "https://github.com/Essent/nativescript-adobe-marketing-cloud",
    "readmeFilename": "README.md",
    "devDependencies": {
        "@nativescript/core": "~7.0.3",
        "@nativescript/types": "~7.0.3",
        "@nativescript/webpack": "^4.1.0",
        "prompt": "^1.0.0",
        "rimraf": "^3.0.2",
        "semver": "7.1.3",
        "ts-patch": "^1.3.0",
        "ts-node": "^9.0.0",
        "tslint": "^6.1.3",
        "typescript": "~4.0.0"
    },
    "dependencies": {},
    "bootstrapper": "nativescript-plugin-seed"
}
